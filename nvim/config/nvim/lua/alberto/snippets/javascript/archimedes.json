{
  "Archimedes Query": {
    "prefix": "ar-query",
    "body": [
      "import { Query } from '@archimedes/arch'",
      "",
      "export class ${1} extends Query<${2:Result},${3:Param}> {",
      "",
      "  constructor(${4}) {",
      "    super()",
      "  }",
      "",
      "  internalExecute(param: ${3:Param}): Promise<${2:Result}> {",
      "    ${5}",
      "  }",
      "}"
    ],
    "description": "Archimedes Query"
  },
  "Archimedes Comman": {
    "prefix": "ar-cmd",
    "body": [
      "import { Command } from '@archimedes/arch'",
      "",
      "export class ${1} extends Command<${3:Param},${2:Result}> {",
      "",
      "  constructor(${4}) {",
      "    super()",
      "  }",
      "",
      "  internalExecute(param: ${3:Param}): Promise<${2:Result}> {",
      "    ${5}",
      "  }",
      "}"
    ],
    "description": "Archimedes Command"
  }
}
